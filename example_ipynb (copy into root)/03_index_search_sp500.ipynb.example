    },
    {
      "cell_type": "code",
      "execution_count": 2,
      "id": "17a40013",
      "metadata": {},
      "outputs": [],
      "source": [
        "index_name = \"sp500\"  # options: sp500, nasdaq100, dow30\n",
        "limit = 500  # reduce for quicker scans\n",
        "include_fundamentals = True\n",
        "include_sentiment = True  # True requires API keys and adds latency\n",
        "ticker_cache_dir = \"cache\"\n",
        "ticker_cache_max_age_hours = 24\n"
